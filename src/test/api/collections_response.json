				"dataset_id": "CMS: Vegetative Lifeform Cover from Landsat SR for CONUS, 1984-2018",
				"entry_id": "Continuous_Lifeform_Maps_CONUS_1809_1",
				"has_spatial_subsetting": false,
				"has_transforms": false,
